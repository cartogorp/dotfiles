{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "display_host": true,
            "host_color": "#5c6bc0",
            "postfix": " ",
            "prefix": "",
            "user_color": "#5c6bc0"
          },
          "foreground": "#80cbc4",
          "background": "#221a28",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "folder_icon": "\uf07c ",
            "home_icon": "~",
            "separator_icon": " \ue0b1 ",
            "style": "full"
          },
          "foreground": "#18131e",
          "background": "#7e57c2",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0",
            "clean_color": "#0e0b10",
            "dirty_color": "#c7363f",
            "display_branch_icon": true,
            "display_stash_count": true,
            "display_status": true,
            "display_upstream_icon": true
          },
          "foreground": "#0e0b10",
          "background": "#4db6ac",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "prefix": " \uf017 ",
            "time_format": "15:04"
          },
          "foreground": "#ffb74d",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": " ",
            "prefix": " ",
            "text": "‚ùØ"
          },
          "foreground": "#ce93d8",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
