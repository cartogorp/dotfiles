{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#80cbc4",
          "background": "#0d0d0d",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#ff5370",
          "background": "#0d0d0d",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Path }} ",
          "foreground": "#a1efe4",
          "background": "#0d0d0d",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#80cbc4>\ue0b1 </>{{ .HEAD }} ",
          "foreground": "#ffd54f",
          "background": "#0d0d0d",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue0b0",
          "foreground": "#0d0d0d",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "template": " {{ .UserName }}@{{ .HostName }} <#80cbc4>\ue0b3</> ",
          "foreground": "#ffd54f",
          "background": "#0d0d0d",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "#ffd54f",
          "background": "#0d0d0d",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#aeea00",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff5252{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
